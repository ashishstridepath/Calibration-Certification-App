<div class="page-header">
  <%= link_to test_weights_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_test_weight_path(@test_weight), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show test_weight</h1>
</div>

<dl class="dl-horizontal">
  <dt>Set Name:</dt>
  <dd><%= @test_weight.name %></dd>
  <dt>Certificate Number:</dt>
  <dd><%= @test_weight.certificate_number %></dd>

  <dt>Date Calibrated:</dt>
  <dd><%= @test_weight.date_calibrated %></dd>

</dl>
